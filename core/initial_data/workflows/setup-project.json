{
  "id": "system:setup-project",
  "name": "Setup New Project",
  "description": "Initialize a new project with essential setup tasks",
  "version": "1.0.0",
  "content": {
    "id": "setup-project",
    "name": "Setup New Project",
    "tasks": [
      {
        "id": "task1",
        "name": "Create project directory",
        "function": {
          "name": "cli_command",
          "input": {
            "command": "mkdir",
            "args": ["-p", "{{project_directory}}"]
          }
        },
        "next_tasks": [
          {
            "id": "task2",
            "name": "Initialize git repository",
            "function": {
              "name": "cli_command",
              "input": {
                "command": "git",
                "args": ["init"]
              }
            },
            "next_tasks": [
              {
                "id": "task3",
                "name": "Create README",
                "function": {
                  "name": "cli_command",
                  "input": {
                    "command": "echo",
                    "args": ["# {{project_name}}", ">", "README.md"]
                  }
                },
                "next_tasks": []
              }
            ]
          }
        ]
      }
    ]
  }
}
